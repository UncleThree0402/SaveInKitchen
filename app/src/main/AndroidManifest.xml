<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.unclethree.saveinkitchen">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/save_in_kitchen"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/save_in_kitchen_rounded"
            android:supportsRtl="true"
            android:theme="@style/Theme.SaveInKitchen">
        <activity android:name=".AddPrepareRecipeIngredientActivity">
        </activity>
        <activity android:name=".PrepareRecipeActivity">
        </activity>
        <activity android:name=".CookDishRecipeActivity">
        </activity>
        <activity android:name=".AddFoodActivity">
        </activity>
        <activity android:name=".RecipeActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>